{
    "name": "zendframework/zend-eventmanager",
    "description": "Event Manager is a flexible and fast component designed for subject/observers patterns",
    "license": "BSD-3-Clause",
    "keywords": [
        "zf3",
        "eventmanager"
    ],
    "homepage": "https://github.com/zendframework/zend-event-manager",
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Zend\\EventManager\\": "src/"
        }
    },
    "require": {
        "php": ">=5.5"
    },
    "suggest": {
        "zendframework/zend-servicemanager": "Version ~3.0 to enable lazy listeners"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev",
            "dev-develop": "3.1-dev"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZendTest\\EventManager\\": "test/"
        }
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "1.7.*",
        "satooshi/php-coveralls": "dev-master",
        "phpunit/phpunit": "~4.6"
    }
}